{
  "name": "@iamsupercb/typescript",
  "version": "1.0.0",
  "description": "Template Package",
  "packageManager": "yarn@4.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn workspaces foreach --all -t run build",
    "dev": "nodemon $0",
    "test": "yarn workspaces foreach --all -t run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/IamSuperCB/fom.git",
    "directory": "typescript"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest",
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "TypeScript",
    "Template"
  ],
  "author": "Carlos A. Betancourt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IamSuperCB/MultipleLanguageLibaryTemplate/issues"
  },
  "homepage": "https://github.com/IamSuperCB/MultipleLanguageLibaryTemplate#readme",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@iamsupercb/common": "workspace:^",
    "@iamsupercb/jupyter": "workspace:^",
    "@iamsupercb/jwt": "workspace:^",
    "@iamsupercb/msal": "workspace:^",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.3",
    "axios": "^1.6.3",
    "jest": "^29.7.0",
    "lerna": "^8.0.1",
    "log4js": "^6.9.1",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false,
    "endOfLine": "lf"
  }
}
